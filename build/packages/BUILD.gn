import("//build/components.gni")

group("soliloquy_image") {
  testonly = false
  deps = [
    "//product:soliloquy",
  ]
  
  metadata = {
    # Mark this as an image-producing target
    images = [
      {
        name = "soliloquy"
        type = "zbi"
        path = "obj/build/images/soliloquy/soliloquy.zbi"
      },
    ]
  }
}

group("packages") {
  testonly = false
  deps = [
    ":soliloquy_image",
    "soliloquy_shell:soliloquy_shell_pkg",
  ]
}
