{
    program: {
        runner: "elf_test_runner",
        binary: "bin/soliloquy_shell_test",
    },
    children: [
        {
            name: "mock_flatland",
            url: "#meta/mock_flatland.cm",
        },
        {
            name: "mock_touch_source",
            url: "#meta/mock_touch_source.cm",
        },
    ],
    capabilities: [
        {
            protocol: "fuchsia.test.Suite",
        },
    ],
    use: [
        {
            protocol: [
                "fuchsia.ui.composition.Flatland",
                "fuchsia.ui.pointer.TouchSource",
                "fuchsia.ui.app.ViewProvider",
            ],
        },
        {
            protocol: "fuchsia.logger.LogSink",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.ui.composition.Flatland",
                "fuchsia.ui.pointer.TouchSource",
                "fuchsia.ui.app.ViewProvider",
            ],
            from: "self",
            to: [ "#soliloquy_shell" ],
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "self",
        },
    ],
}
